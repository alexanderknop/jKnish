class Pair {
    construct of(a, b) {
        _x = a;
        _y = b;
    }

    x {
        return _x;
    }

    x=(a) {
        _x = a;
    }

    y {
        return _y;
    }

    y=(a) {
        _y = a;
    }

    toString {
        return "(" + _x.toString + ", " + _y.toString + ")";
    }
}
var p = Pair.of(1, "hello");
System.print(p);
p.x = p.x + 1;
p.y = 1;
System.print(p);
